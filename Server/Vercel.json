{
    "name":"justice-clothing",
    "version": 2,
    "builds": [
        {
            "src": "./dist/index.js",
            "use": "@now/node",
            "config": { "includeFiles": ["dist/**","dist"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "destination": "/index.js"
        }
    ]
}
